<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}La Clémentine{% endblock %}</title>
    <meta name=viewport content="width=device-width, initial-scale=1">

    {% block stylesheets %}
        <link href="https://fonts.googleapis.com/css?family=Open+Sans|Pacifico|Roboto" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link href="{{ asset('build/css/sui.css') }}" rel="stylesheet">
        <link href="{{ asset('build/css/front.css') }}" rel="stylesheet">
        <link href="{{ asset('build/css/main.css') }}" rel="stylesheet">

    {% endblock %}

</head>
<body >
<script type='text/javascript' src='https://maps.googleapis.com/maps/api/js?key=AIzaSyD8VwmSjvYXfjvMFZ9lH2-6vlGI_0-gjS0'></script>
<div id="products" data-vars="{{ products|json_encode()|e('html_attr') }}"></div>
<div id="site" data-vars="{{ site|json_encode()|e('html_attr') }}"></div>

    <div id="app" style="width: 100%">

    </div>

{% block javascripts %}
      <script src="{{ asset('build/manifest.json') }}"></script>
      <script src="{{ asset('build/main.js') }}"></script>

     {# <script src="{{ asset('build/js/app.js') }}"></script> #}
{% endblock %}

<!--<script  src=”https://maps.googleapis.com/maps/api/js?key=AIzaSyD8VwmSjvYXfjvMFZ9lH2-6vlGI_0-gjS0” async defer ></script>-->
</body>
</html>